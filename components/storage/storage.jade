template(name="Storage")
    //-
        Overview of items corporation own

    .storage.panel.component(style="display:none")
        .panel-heading
            .pull-left Storage

        .panel-body

            .storage-window
                each storageList
                    .storage-item(data-context="item" quantity="#{qty}" itemId="#{id}")
                        .placeholder-icon.pull-left
                        .pull-left #{name} x#{qty}
